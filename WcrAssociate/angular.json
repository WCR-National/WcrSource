{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular7project": {
      "root": "",
      "sourceRoot": "AngularAssociate",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "Scripts/libs",
            "index": "index.html",
            "main": "AngularAssociate/main.ts",
            "polyfills": "AngularAssociate/polyfills.ts",
            "tsConfig": "AngularAssociate/tsconfig.app.json",
            "assets": [
              "AngularAssociate/assets",
              "AngularAssociate/favicon.ico"
            ],
            "styles": [
              "AngularAssociate/styles.css",
              "AngularAssociate/assets/bootstrap.css",
              "AngularAssociate/assets/style.css",
              "AngularAssociate/assets/color.css",
              //"./node_modules/ngx-lightbox/lightbox.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css"
              //"node_modules/select2/dist/css/select2.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/crypto-js/crypto-js.js",
              "AngularAssociate/assets/js/croppie.min.js",
              "AngularAssociate/assets/js/imguploader.js",
              "AngularAssociate/assets/js/mmenu.min.js",
              "AngularAssociate/assets/js/magnific-popup.min.js",
              "AngularAssociate/assets/js/slick.min.js",
              "AngularAssociate/assets/js/main_custom.js",
              "node_modules/select2/dist/js/select2.min.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "AngularAssociate/environments/environment.ts",
                  "with": "AngularAssociate/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "15mb",
                  "maximumError": "20mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular7project:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular7project:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular7project:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "AngularAssociate/test.ts",
            "polyfills": "AngularAssociate/polyfills.ts",
            "tsConfig": "AngularAssociate/tsconfig.spec.json",
            "karmaConfig": "AngularAssociate/karma.conf.js",
            "styles": [
              "AngularAssociate/styles.css"

              //"node_modules/select2/dist/css/select2.min.css"
            ],
            "scripts": [

            ],
            "assets": [
              "AngularAssociate/assets",
              "AngularAssociate/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "AngularAssociate/tsconfig.app.json",
              "AngularAssociate/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "angular7project-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "angular7project:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular7project:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "angular7project"
}